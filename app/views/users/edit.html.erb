<div class="container">
<ul>
	<%= form_for @user do |f| %>
		<li>
			<%= f.label :username %>:
  		<%= f.text_field :username %>
  	</li>

  	<li>
		  <%= f.label :email %>:
		  <%= f.text_field :email %>
		</li>

		<li>
	  	<%= f.label :password %>:
	  	<%= f.password_field :password %>
	  </li>

	  <li>
		  <%= f.label :password_confirmation %>:
		  <%= f.password_field :password_confirmation %>
		</li>
		
	  <%= f.submit %>
	<% end %>
</ul>
</div>